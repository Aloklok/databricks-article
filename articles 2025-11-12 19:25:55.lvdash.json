    {
      "name": "dfab06f6",
      "displayName": "网站运行天数",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT \n",
        "  MIN(n8n_processing_date) AS earliest_date,\n",
        "  DATEDIFF(current_date(), MIN(n8n_processing_date)) AS days_since_earliest\n",
        "FROM `supabase-article_catalog`.public.articles"
      ]
    }
